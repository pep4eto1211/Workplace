<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="using:Syncfusion.UI.Xaml.Controls" xmlns:Media="using:Syncfusion.UI.Xaml.Controls.Media" xmlns:sys="using:System"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" xmlns:shared="using:Syncfusion.UI.Xaml.Converters"
    xmlns:Navigation="using:Syncfusion.UI.Xaml.Controls.Navigation" 
    xmlns:controls="using:Syncfusion.UI.Xaml.Controls"
   >


    <!--Radial Slider template-->

    <Media:ResourceWrapper x:Key="ResourceWrapperKey"></Media:ResourceWrapper>
    
    <Style TargetType="Navigation:RadialPointer" x:Key="PointerStyle">
        <Setter Property="RenderTransformOrigin" Value="0, 0.5"/>
        <Setter Property="Height" Value="30"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Navigation:RadialPointer">
                    <Grid x:Name="PART_Root">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Border  Background="{TemplateBinding Background}" Height="2"/>
                        <Ellipse Fill="{TemplateBinding Background}" Grid.Column="1" 
                                         HorizontalAlignment="Right" Width="30" 
                                         Height="30" Margin="0 0 -10 0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Radial Slider -->

    <Style TargetType="Navigation:RadialList" x:Key="RadialTicksListStyle" >
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <Navigation:RadialPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <Style x:Key="SfRadialSliderStyle" TargetType="Navigation:SfRadialSlider">
        <Setter Property="LabelRadiusFactor" Value="0"/>
        <Setter Property="Width" Value="350"/>
        <Setter Property="Height" Value="350"/>
        <Setter Property="Minimum" Value="0"/>
        <Setter Property="Maximum" Value="255"/>
        <Setter Property="InnerRimRadiusFactor" Value="0.5"/>
        <Setter Property="InnerRimStrokeThickness" Value="2"/>
        <Setter Property="OuterRimRadiusFactor" Value=".5"/>
        <Setter Property="PointerRadiusFactor" Value=".5"/>
        <Setter Property="InnerRimRadiusFactor" Value="0.25"></Setter>
        <Setter Property="Background" Value="Black"/>
        <Setter Property="OuterRimStroke" Value="#FF757575"/>
        <Setter Property="InnerRimFill" Value="White"/>
        <Setter Property="PointerStyle" Value="{StaticResource PointerStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Navigation:SfRadialSlider">
                    <Border Background="Transparent">
                        <Grid>
                            <Navigation:RadialList Visibility="{TemplateBinding TickVisibility}" x:Name="PART_Ticks" Style="{StaticResource RadialTicksListStyle}" />
                            <Ellipse x:Name="PART_OuterEllipse"
        								Stroke="{TemplateBinding OuterRimStroke}" 
        								Fill="{TemplateBinding Background}"
        								StrokeThickness="{TemplateBinding OuterRimStrokeThickness}"
        								HorizontalAlignment="Center" 
        								VerticalAlignment="Center"                                     
                                    />
                            <Navigation:RadialPointer Background="{TemplateBinding InnerRimFill}" Style="{TemplateBinding PointerStyle}"  
                                                      HorizontalAlignment="Right"
                                                      x:Name="PART_Pointer" VerticalAlignment="Center"/>
                            <Ellipse   x:Name="PART_InnerEllipse" 
        								Stroke="{TemplateBinding InnerRimStroke}" 
        								Fill="{TemplateBinding InnerRimFill}" 
        								HorizontalAlignment="Center" 
        								VerticalAlignment="Center"                               
        								StrokeThickness="{TemplateBinding InnerRimStrokeThickness}" />
                            <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" HorizontalAlignment="Center" 
                                                      VerticalAlignment="Center"  x:Name="PART_Content"/>

                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

    </Style>


    <!--ColorPicker Spec-->
    <shared:ColorToBrushConverter x:Key="BrushConverter"/>

    <shared:RGBConverter x:Key="RGBConverter"/>

    <!--ColorSlider style-->
    <Style TargetType="Media:ColorSlider">
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Media:ColorSlider">
                    <Grid x:Name="colorBox">
                        <Rectangle x:Name="PART_ColorBar">
                            <Rectangle.Fill>
                               <ImageBrush x:Name="ColorBarBackground" Stretch="Fill"/>
                            </Rectangle.Fill>
                        </Rectangle>
                    <Canvas Background="Transparent" x:Name="PART_Root" ManipulationMode="All">
                            <Grid x:Name="PART_Thumb" HorizontalAlignment="Left" Margin="-10 0 0 0" Width="20">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="20"/>
                                <RowDefinition Height="10"/>
                            </Grid.RowDefinitions>
                            <Rectangle Width="1.5" Fill="Black"/>
                            <Ellipse Grid.Row="1" Stroke="Black" StrokeThickness="1" Width="20" />
                            <Ellipse Grid.Row="1" Stroke="White" Fill="Transparent" StrokeThickness="1" Width="18" Height="18"
                                     HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <Rectangle Grid.Row="2" Width="1.5" Fill="Black" />
                        </Grid>
                        <!--Tooltip-->
                        <Popup x:Name="PART_Tooltip">
                            <Media:ColorTooltip SelectedColor="Red"/>
                        </Popup>
                    </Canvas>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="Auto"/>
        <Setter Property="Height" Value="40"/>
    </Style>


    <!--ColorSelectionBox style-->
    <Style TargetType="Media:ColorSelectionBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border>
                        <Grid x:Name="ColorBox">
                            <Rectangle x:Name="ColorRect"  RenderTransformOrigin="0.5, 0.5">
                                <Rectangle.RenderTransform>
                                    <RotateTransform Angle="180"/>
                                </Rectangle.RenderTransform>
                                <Rectangle.Fill>
                                    <ImageBrush x:Name="PART_ImageBrush"/>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Canvas x:Name="PART_Canvas" Background="Transparent">

                                <!--Thumb-->
                                <Grid Background="Transparent" 
                                      x:Name="PART_Thumb" Height="45" Width="45">
                                    <Grid Background="Transparent" 
                                          Width="25" Height="25"
                                          HorizontalAlignment="Center" 
                                          VerticalAlignment="Center">
                                        <Rectangle Stroke="Black" StrokeThickness="1" />
                                        <Rectangle Stroke="White" StrokeThickness="1" Margin="1"
                                                   Fill="Transparent"/>
                                    </Grid>
                                </Grid>

                                <!--Tooltip-->
                                <Popup x:Name="PAT_Tooltip">
                                    <Media:ColorTooltip />
                                </Popup>

                            </Canvas>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <DataTemplate x:Key="RSliderContentTemplate">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="{Binding}" FontSize="22" Foreground="Black"/>
            <TextBlock Text="{Binding Source={StaticResource ResourceWrapperKey},Path=R}" FontWeight="Bold" VerticalAlignment="Top" FontSize="10" Foreground="Black"/>
        </StackPanel>
    </DataTemplate>

    <DataTemplate x:Key="GSliderContentTemplate">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="{Binding}" FontSize="22" Foreground="Black"/>
            <TextBlock Text="{Binding Source={StaticResource ResourceWrapperKey},Path=G}" FontWeight="Bold" VerticalAlignment="Top" FontSize="10" Foreground="Black"/>
        </StackPanel>
    </DataTemplate>

    <DataTemplate x:Key="BSliderContentTemplate">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="{Binding}" FontSize="22" Foreground="Black"/>
            <TextBlock Text="{Binding Source={StaticResource ResourceWrapperKey},Path=B}" FontWeight="Bold" VerticalAlignment="Top" FontSize="10" Foreground="Black"/>
        </StackPanel>
    </DataTemplate>

    <!--SfColorPicker-->
    <Style TargetType="Media:SfColorPicker">
        <Setter Property="Width" Value="380"/>
        <Setter Property="Height" Value="300"/>
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="Background" Value="Black"/>
        <Setter Property="BorderBrush" Value="#5F5F5F"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Media:SfColorPicker">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}">
                        <Grid Margin="6 6 0 6">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="40"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="100"/>
                            </Grid.ColumnDefinitions>
                            <Media:ColorSelectionBox x:Name="PART_Box" Hue="{Binding ElementName=PART_Slider, Path=Value}"/>
                            <Media:ColorSlider Grid.Row="1" x:Name="PART_Slider"/>
                            <Grid Grid.Column="1" Grid.RowSpan="2">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Border BorderThickness="0 0 0 1" BorderBrush="#5F5F5F">
                                    <TextBlock Foreground="#5F5F5F" Text="{Binding Source={StaticResource ResourceWrapperKey},Path=RGB}" Margin="5" FontSize="20"
                                               HorizontalAlignment="Center"/>
                                </Border>
                                <Border BorderThickness="0 0 0 1" Background="Transparent" 
                                        BorderBrush="#5F5F5F" Grid.Row="1" x:Name="PART_R">
                                    <Grid>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                                            <TextBlock Text="{Binding Path=SelectedColor, 
                                                    RelativeSource={RelativeSource TemplatedParent},
                                                    Mode=TwoWay, Converter={StaticResource RGBConverter},
                                                    ConverterParameter=R}" FontSize="22" Foreground="#FFEC1E24"/>
                                            <TextBlock Text="{Binding Source={StaticResource ResourceWrapperKey},Path=R}" VerticalAlignment="Top" FontWeight="Bold" FontSize="10" Foreground="#FFEC1E24"/>
                                        </StackPanel>
                                        <Popup x:Name="PART_RPopup">
                                            <Grid>
                                                <Navigation:SfRadialSlider x:Name="PART_RSlider"
                                                    ContentTemplate="{StaticResource RSliderContentTemplate}"
                                                    Style="{StaticResource SfRadialSliderStyle}"
                                                    TickFrequency="1" SmallChange="1" LabelVisibility="Collapsed"
                                                    TickVisibility="Collapsed"
                                                    Value="0" Content="{Binding Path=SelectedColor, 
                                                    RelativeSource={RelativeSource TemplatedParent},
                                                    Mode=TwoWay, Converter={StaticResource RGBConverter},
                                                    ConverterParameter=R}"
                                                    InnerRimStroke="#EF6B6B"
                                                    InnerRimFill="#FFEC1E24" 
                                                    />
                                            </Grid>
                                        </Popup>
                                    </Grid>
                                </Border>
                                <Border BorderThickness="0 0 0 1" Background="Transparent" BorderBrush="#5F5F5F" Grid.Row="2" x:Name="PART_G">
                                    <Grid>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                                            <TextBlock Text="{Binding Path=SelectedColor, 
                                                    RelativeSource={RelativeSource TemplatedParent},
                                                    Mode=TwoWay, Converter={StaticResource RGBConverter},
                                                    ConverterParameter=G}" FontSize="22" Foreground="#FF6CBD45"/>
                                            <TextBlock Text="{Binding Source={StaticResource ResourceWrapperKey},Path=G}" FontWeight="Bold" VerticalAlignment="Top" FontSize="10" Foreground="#FF6CBD45"/>
                                        </StackPanel>
                                        <Popup x:Name="PART_GPopup">
                                            <Grid>
                                                <Navigation:SfRadialSlider x:Name="PART_GSlider"
                                                    ContentTemplate="{StaticResource GSliderContentTemplate}"
                                                    Style="{StaticResource SfRadialSliderStyle}"
                                                    TickVisibility="Collapsed"
                                                    LabelVisibility="Collapsed"
                                                    TickFrequency="1" SmallChange="1"
                                                    Value="0" Content="{Binding Path=SelectedColor, 
                                                    RelativeSource={RelativeSource TemplatedParent},
                                                    Mode=TwoWay, Converter={StaticResource RGBConverter},
                                                    ConverterParameter=G}"
                                                    InnerRimStroke="Green"
                                                    InnerRimFill="#FF6CBD45" 
                                                    />
                                            </Grid>
                                        </Popup>
                                    </Grid>
                                </Border>
                                <Border BorderThickness="0 0 0 1" Background="Transparent" BorderBrush="#5F5F5F" Grid.Row="3" x:Name="PART_B">
                                    <Grid>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                                            <TextBlock Text="{Binding Path=SelectedColor, 
                                                    RelativeSource={RelativeSource TemplatedParent},
                                                    Mode=TwoWay, Converter={StaticResource RGBConverter},
                                                    ConverterParameter=B}" FontSize="22" Foreground="#FF488DCB"/>
                                            <TextBlock Text="{Binding Source={StaticResource ResourceWrapperKey},Path=B}" FontWeight="Bold" VerticalAlignment="Top" FontSize="10" Foreground="#FF488DCB"/>
                                        </StackPanel>
                                        <Popup x:Name="PART_BPopup">
                                            <Grid>
                                                <Navigation:SfRadialSlider x:Name="PART_BSlider"
                                                    ContentTemplate="{StaticResource BSliderContentTemplate}"
                                                    Style="{StaticResource SfRadialSliderStyle}"
                                                    TickVisibility="Collapsed"
                                                    LabelVisibility="Collapsed"
                                                    TickFrequency="1" SmallChange="1"
                                                    Value="0" Content="{Binding Path=SelectedColor, 
                                                    RelativeSource={RelativeSource TemplatedParent},
                                                    Mode=TwoWay, Converter={StaticResource RGBConverter},
                                                    ConverterParameter=B}"
                                                    InnerRimStroke="Blue"
                                                    InnerRimFill="#FF488DCB" 
                                                    />
                                            </Grid>
                                        </Popup>
                                    </Grid>
                                </Border>
                                <Border BorderThickness="0 0 0 1" BorderBrush="#5F5F5F" Grid.Row="4">
                                    <RichTextBlock x:Name="PART_Textbox" 
                                               HorizontalAlignment="Center" 
                                               Foreground="#5F5F5F" Margin="3 5" FontSize="17"
                                               VerticalAlignment="Center">
                                        <Paragraph>
                                            <Run Text="{Binding SelectedColor, RelativeSource={RelativeSource TemplatedParent}}"/>
                                        </Paragraph>
                                    </RichTextBlock>
                                </Border>
                                <Grid Grid.Row="5" HorizontalAlignment="Center"
                                      VerticalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Grid Margin="-10 0 0 0"  Grid.Column="1">
                                        <Ellipse Width="25" Height="25" Stroke="Black" StrokeThickness="0.5"
                                                 Fill="#5F5F5F" HorizontalAlignment="Center"
                                                 VerticalAlignment="Center"/>
                                        <Ellipse Width="21" Height="21" Stroke="Black"
                                                 HorizontalAlignment="Center"
                                                 VerticalAlignment="Center"
                                                 StrokeThickness="1" Fill="{Binding Path=PreviousColor, 
                                                 RelativeSource={RelativeSource TemplatedParent}, 
                                                 Converter={StaticResource BrushConverter}}"/>
                                    </Grid>
                                    <Grid>
                                        <Ellipse Width="40" Height="40" Fill="#5F5F5F" HorizontalAlignment="Center"
                                                 VerticalAlignment="Center"
                                                  Stroke="Black" StrokeThickness="0.5"/>
                                        <Ellipse Width="36" Height="36" Stroke="Black"
                                                 HorizontalAlignment="Center"
                                                 VerticalAlignment="Center"
                                                 StrokeThickness="1" Fill="{Binding Path=SelectedColor, 
                                                 RelativeSource={RelativeSource TemplatedParent}, 
                                                 Converter={StaticResource BrushConverter}}"/>
                                    </Grid>

                                </Grid>
                            </Grid>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="Media:ColorTooltip">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Media:ColorTooltip">
                    <Grid >
                        <Path Data="M45,0 C69.4645,0 89.3693,19.5225 89.9853,43.8385 L89.9906,44.2596 L90,44.25 L90,45 L90,90 L45.375,90 L45.3797,89.9952 L45,90 C20.1472,90 0,69.8528 0,45 C0,20.1472 20.1472,0 45,0 z" Fill="Black" HorizontalAlignment="Center" Height="90" Stretch="Fill" UseLayoutRounding="False" VerticalAlignment="Center" Width="90" Stroke="White" StrokeThickness="0.5"/>
                        <Ellipse Fill="{Binding SelectedColor, Converter={StaticResource BrushConverter}, RelativeSource={RelativeSource Mode=TemplatedParent}}" Stroke="#00C3BEBE"
                                 HorizontalAlignment="Center" Height="75" StrokeThickness="1"
                                 VerticalAlignment="Center" Width="75" Margin="0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="90"/>
        <Setter Property="Height" Value="90"/>
    </Style>

    <!--ColorSwatches Spec-->

    <Style TargetType="Media:ColorSwatches">
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Media:ColorSwatches">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" VerticalScrollMode="Auto" HorizontalScrollMode="Auto" IsZoomChainingEnabled="False">
                        <Grid x:Name="PART_Background" Background="#F7F7F7">
                            <Grid.Resources>
                                <Style TargetType="Media:ColorItem">
                                    <Setter Property="VerticalAlignment" Value="Stretch"/>
                                    <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                </Style>
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item0" Tag="Apex"  Grid.Row="0" Grid.Column="0" Background="White" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#635672"/>
                                    <Rectangle Fill="#746425" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#DDD9E2" Grid.Row="1"/>
                                    <Rectangle Fill="#EAE2C0" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item1" Tag="Hardcover" Grid.Row="0" Grid.Column="1" Background="#FFF2F2F2" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#D3735E"/>
                                    <Rectangle Fill="#F25838" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#EFCFC8" Grid.Row="1"/>
                                    <Rectangle Fill="#FAC6BB" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item2" Tag="Metro" Grid.Row="0" Grid.Column="2" Background="#FFD8D8D8" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#5EA126"/>
                                    <Rectangle Fill="#FDB70A" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#CAEBAF" Grid.Row="1"/>
                                    <Rectangle Fill="#FDD36B" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item3" Tag="Module" Grid.Row="1" Grid.Column="0" Background="#FFBFBFBF" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#E78551"/>
                                    <Rectangle Fill="#60B4CB" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#F0B595" Grid.Row="1"/>
                                    <Rectangle Fill="#9ED1DF" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item4" Tag="Office" Grid.Row="1" Grid.Column="1" Background="#FFA5A5A5" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#F69546"/>
                                    <Rectangle Fill="#BF504D" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#F9BF8E" Grid.Row="1"/>
                                    <Rectangle Fill="#D89593" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item5" Tag="Paper" Grid.Row="1" Grid.Column="2" Background="#FF7F7F7F" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#DECD04"/>
                                    <Rectangle Fill="#71539F" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#C2B4D8" Grid.Row="1"/>
                                    <Rectangle Fill="#FAEE59" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item6" Tag="Pushpin" Grid.Row="2" Grid.Column="0" Background="Black" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#5C650C"/>
                                    <Rectangle Fill="#D57B01" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#FCC57A" Grid.Row="1"/>
                                    <Rectangle Fill="#DEEA69" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item7" Tag="Solstice" Grid.Row="2" Grid.Column="1" Background="#FF7F7F7F" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#3890A6"/>
                                    <Rectangle Fill="Black" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#BEBEBE" Grid.Row="1"/>
                                    <Rectangle Fill="#DF7B7C" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item8" Tag="Urban" Grid.Row="2" Grid.Column="2" Background="#FF595959" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#78397A"/>
                                    <Rectangle Fill="#68452D" Grid.Row="1"  Grid.Column="1"/>
                                    <Rectangle Fill="#9192BC" Grid.Row="1"/>
                                    <Rectangle Fill="#9192BC" Grid.Column="1"/>
                                </Grid>
                            </Media:ColorItem>
                            <Media:ColorItem TooltipVisibility="Collapsed" x:Name="PART_Item9" Tag="Waveform" Grid.Row="3" Grid.Column="0" Background="#FF3F3F3F" Margin="{TemplateBinding ColorItemMargin}" Height="{TemplateBinding ColorItemWidth}" Width="{TemplateBinding ColorItemWidth}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Fill="#2861A8"/>
