<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Syncfusion.UI.Xaml.Charts">
        
<!--Legend shapes according to Symbols-->
    <DataTemplate x:Key="Pentagon">
        <Polyline Points="7.5,0,15,7,12,15,3,15,0,7,7.5,0" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Square">
        <Rectangle Height="15" Width="15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Rectangle">
        <Rectangle Height="15" Width="15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Circle">
        <Ellipse Width="15" Height="15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Triangle">
        <Polyline Points="7.5,0,15,15,0,15,7.5,0" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="InvertedTriangle">
        <Polyline Points="0,0,15,0,7.5,15,0,0" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StraightLine">
        <Polyline Points="0,8,15,8" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Diamond">
        <Polyline Points="7.5,0,15,7.5,7.5,15,0,7.5,7.5,0" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Cross">
        <Polyline Points="7.5,0, 7.5,15, 7.5,8, 0,8, 15,8, 7.5,8" Height="15" Width="15" Fill="{Binding Series.Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>

    <!--Legend shapes according to Chart Types-->
    <DataTemplate x:Key="Bar">
        <Rectangle Width="15" Height="15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Area">
        <Polyline Points="0,3,4,6,10,2,15,6,15,15,0,15,0,3" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="BoxAndWhisker">
        <Polyline Points="1,0, 11,0, 7.5,0, 7.5,3, 7,3, 15,3, 15,12, 2,12, 2,3, 7.5,3, 2,3, 2,12, 7.5,12, 7.5,15, 6,15, 11,15, 7.5,15, 7.5,12, 2,12, 2,3, 7.5,3, 7.5,0"
                  Fill="{Binding Series.Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Bubble">
        <Ellipse Height="15" Width="15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="FastLine">
        <Polyline Points="0,0,5,10,10,1,15,15,10,1,5,10,0,0" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="FastLineBitmap">
        <Polyline Points="0,0,5,10,10,1,15,15,10,1,5,10,0,0" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="FastLineDirectX">
        <Polyline Points="0,0,5,10,10,1,15,15,10,1,5,10,0,0" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Candle">
        <Polyline Points="7.5,0, 7.5,5, 0,5, 0,15, 15,15, 15,5, 7.5,5" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Funnel">
        <Polyline Points="0,0, 4,15, 11,15, 15,0,0,0" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="HiLo">
        <Polyline Points="0,0, 0,15" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="HiLoOpenClose">
        <Polyline Points="7.5,0, 7.5,15, 0,15, 7.5,15, 7.5,7.5, 15,7.5, 7.5,7.5, 7.5,0" Stroke="{Binding Series.Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Kagi">
        <Polyline Points="0,15, 9,15, 9,0, 15,0, 9,0, 9,15, 0,15" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Column">
        <Polyline Points="0,15, 0,0, 10,0, 10,15,0,15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="FastColumnBitmap">
        <Polyline Points="0,15, 0,0, 10,0, 10,15,0,15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Gantt">
        <Polyline Points="0,12, 0,3, 15,3, 15,12, 0,12 " Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Histogram">
        <Polyline Points="0,8, 5,8, 5,4, 10,4, 10,10, 15,10, 15,15, 0,15, 0,8" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Line">
        <Polyline Points="0,10,5,10,10,1,15,15,10,1,5,10,0,10" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Pyramid">
        <Polyline Points="3,5, 12,5, 15,15, 0,15, 3,5" Fill="{Binding nterior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="PointAndFigure">
        <Ellipse Width="15" Height="10" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="RangeArea">
        <Polyline Points="0,0, 8,6, 15,0, 15,15, 5,10, 0,15, 0,0" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="RangeColumn">
        <Polyline Points="4,0, 11,0, 11,15, 4,15, 4,0" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Renko">
        <Polyline Points="0,0, 5,10, 10,5, 15,10, 10,5, 5,10" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Scatter">
        <Ellipse Height="8" Width="8" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"></Ellipse>
    </DataTemplate>
    <DataTemplate x:Key="FastScatter">
        <Ellipse Height="8" Width="8" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"></Ellipse>
    </DataTemplate>
    <DataTemplate x:Key="Spline">
        <Path Data="M 0,10 C 0,10 3,3 7,7 C 7,7 11,11 15,4 C 15,4 11,11 7,7 C 7,7 3,3 0,10" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="SplineArea">
        <Path Data="M 0,7 C 0,7 2.5,10 5,7 C 5,7 7.5,4 10,7 C 10,7 12.5,10 15,7 L 15,15 L 0,15 L 0,7" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="RotatedSpline">
        <Path Data="M 4,0 C 4,0 11,5 7,7 C 7,7 4,10 11,15 C 11,15 4,10 7,7 C 7,7 11,5 4,0" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StackingArea">
        <Polyline Points="0,10, 5,2, 10,10, 15,7, 15,15, 0,15, 0,10" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StackingBar">
        <Polyline Points="0,4, 10,4, 10,12, 0,12, 0,4" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StackingBar100">
        <Polyline Points="0,4, 15,4, 15,12, 0,12, 0,4" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StackingColumn">
        <Polyline Points="4,15, 4,5, 10,5, 10,15, 4,15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StackingColumn100">
        <Polyline Points="4,15, 4,0, 10,0, 10,15, 4,15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StepArea">
        <Polyline Points="0,7, 5,7, 5,3, 10,3, 10,10, 15,10, 15,15, 0,15, 0,7" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="StepLine">
        <Polyline Points="0,7, 5,7, 5,3, 10,3, 10,10, 15,10, 10,10, 10,3, 5,3, 5,7, 0,7" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="ThreeLineBreak">
        <Rectangle Height="15" Width="15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Tornado">
        <Rectangle Height="10" Width="15" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Radar">
        <Polyline Points="5,3, 15,8, 10,15, 3,15, 0,6, 5,3, 5,3, 0,6, 3,15, 10,15, 15,8, 5,3" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Polar">
        <Polyline Points="5,3, 15,8, 10,15, 3,15, 0,6, 5,3, 5,3, 0,6, 3,15, 10,15, 15,8, 5,3" Stroke="{Binding Interior}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Pie">
        <Path Data="M 0,4 C 0,4 7,0 15,4 L 7.5,15 L 0,4" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    <DataTemplate x:Key="Doughnut">
        <Path Data="M 0,4 C 0,4 7,0 15,4 L 10,15 C 10,15 7.5,12 5,15 L 0,4" Fill="{Binding Interior}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}"/>
    </DataTemplate>
    
</ResourceDictionary>

